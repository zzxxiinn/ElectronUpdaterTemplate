{
  "name": "electron-updater-template",
  "version": "0.1.0",
  "private": true,
  "main": "background.js",
  "scripts": {
    "build": "vue-cli-service electron:build -p never",
    "build-all": "vue-cli-service electron:build -p never -mwl",
    "build-mac": "vue-cli-service electron:build -p never -m",
    "build-win": "vue-cli-service electron:build -p never -w",
    "build-linux": "vue-cli-service electron:build -p never -l",
    "build-publish": "vue-cli-service electron:build -p always",
    "serve": "vue-cli-service electron:serve --trace-warnings",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "3.25.2",
    "electron": "15.0.0",
    "electron-log": "4.4.8",
    "electron-updater": "4.6.5",
    "vue": "3.2.39"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@vue/cli-plugin-babel": "4.5.19",
    "@vue/cli-plugin-eslint": "4.5.19",
    "@vue/cli-plugin-typescript": "4.5.19",
    "@vue/cli-service": "4.5.19",
    "@vue/compiler-sfc": "3.2.39",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "7.0.0",
    "eslint": "6.8.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "7.20.0",
    "less": "3.13.1",
    "less-loader": "5.0.0",
    "prettier": "2.7.1",
    "typescript": "4.1.6",
    "vue-cli-plugin-electron-builder": "2.1.1"
  }
}

{
  "name": "electron-updater-template",
  "version": "0.1.0",
  "private": true,
  "main": "background.js",
  "scripts": {
    "build": "vue-cli-service electron:build -p never",
    "build-all": "vue-cli-service electron:build -p never -mwl",
    "build-mac": "vue-cli-service electron:build -p never -m",
    "build-win": "vue-cli-service electron:build -p never -w",
    "build-linux": "vue-cli-service electron:build -p never -l",
    "build-publish": "vue-cli-service electron:build -p always",
    "serve": "vue-cli-service electron:serve --trace-warnings",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "electron": "13.1.9",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.3.9",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "prettier": "^2.2.1",
    "typescript": "~4.1.5",
    "vue-cli-plugin-electron-builder": "^2.1.1"
  }
}
